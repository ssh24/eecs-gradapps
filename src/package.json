{
  "name": "gradapps",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --fix",
    "start": "nodemon ./bin/www",
    "dev:start": "npm run seed:database && npm start",
    "seed:database": "node ./test/pretest.js",
    "test": "nyc --reporter=html --reporter=text npm run run:test",
    "run:test": "npm run seed:database && mocha --timeout 5000 test/specs/database/*.test.js && webdriver-manager update && protractor config.js",
    "posttest": "npm run lint",
    "db:test": "npm run seed:database && nyc --reporter=text --reporter=html mocha test/specs/database/*.test.js && npm run posttest",
    "unit:test": "npm run seed:database && webdriver-manager update && nyc --reporter=text --reporter=html protractor config.js && npm run posttest"
  },
  "dependencies": {
    "apache-crypt": "^1.2.1",
    "assert": "^1.4.1",
    "body-parser": "^1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "ejs": "^2.5.7",
    "express": "~4.15.5",
    "express-mysql-session": "^1.2.3",
    "express-session": "^1.15.6",
    "http-auth": "^3.2.3",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "ms": "^2.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.13.1",
    "htpasswd": "^2.4.0",
    "mocha": "^4.1.0",
    "mysql2": "^1.5.1",
    "nodemon": "^1.14.12",
    "nyc": "^11.4.1",
    "protractor": "^5.2.2"
  }
}
